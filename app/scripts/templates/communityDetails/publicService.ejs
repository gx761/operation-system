<div class="service_box_inner">

    <div class="service_title <%= community_id ?'active':'inactive'%>"><%=name%></div>
    <div class="service_toggle">
        <% if(community_id ){%>
        <a class='stop_service'>暂停服务</a>
        <%} else{%>
        <a class='start_service'>启动服务</a>
        <%}%>
    </div>
</div>