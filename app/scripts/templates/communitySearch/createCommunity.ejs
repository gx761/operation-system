<div class="modal-dialog">
	<div class="modal-content">
		<form action="" class="form-horizontal" id="create_community_form">
			<div class="form-group">
			<div class="col-xs-3">
				<select name="country" id="new_country_selector" class="form-control col-xs-3">
					<option value=''>国家</option>
				</select>
			</div>
			

			
			<div class="col-xs-3">
				<select name="province" id="new_province_selector" class="form-control col-xs-3">
					<option value=''>省份</option>
				</select>
				</div>
						
			<div class="col-xs-3">
				<select name="city" id="new_city_selector" class="form-control col-xs-3">
					<option value=''>城市</option>
				</select>
				</div>
			
			
			<div class="col-xs-3">
				<select name="district" id="new_district_selector" class="form-control col-xs-3">
					<option value=''>区域</option>
				</select>		
			</div>
			</div>

			<div class="form-group">
			<div class="col-xs-6">
			<input type="text" class="form-control" name="longitude" id="longitude" placeholder="经度">
			</div>
			<div class="col-xs-6">
			<input type="text" class="form-control" name="latitude" id="latitude" placeholder="纬度">
			</div>
			</div>

			<div class="form-group">
			<div class="col-xs-12">
				<input type="text" class="form-control" name="小区" placeholder="小区">
				</div>
			</div>
			<div class="form-group">
			<div class="col-xs-12">
				<input type="text" class="form-control" name="小区物业" placeholder="小区物业">
				</div>
			</div>
			<div class="form-group">	
			<div class="col-xs-12">
				<input type="text" class="form-control" name="小区物业人员手机号" placeholder="小区物业人员手机号">
				</div>
			</div>

		</form>
	</div>
</div>